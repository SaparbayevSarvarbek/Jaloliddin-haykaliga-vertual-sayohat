<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.jaloliddinhaykaligavertualsayohat.fragment.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registationFragmen2"
            app:destination="@id/registationFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_adminFragment"
            app:destination="@id/adminFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.jaloliddinhaykaligavertualsayohat.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/registationFragment"
        android:name="com.example.jaloliddinhaykaligavertualsayohat.fragment.RegistationFragment"
        android:label="fragment_registation"
        tools:layout="@layout/fragment_registation" >
        <action
            android:id="@+id/action_registationFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/adminFragment"
        android:name="com.example.jaloliddinhaykaligavertualsayohat.fragment.AdminFragment"
        android:label="fragment_admin"
        tools:layout="@layout/fragment_admin" >
        <action
            android:id="@+id/action_adminFragment_to_monumentDataFragment"
            app:destination="@id/monumentDataFragment" />
        <action
            android:id="@+id/action_adminFragment_to_personDataFragment"
            app:destination="@id/personDataFragment" />
    </fragment>
    <fragment
        android:id="@+id/monumentDataFragment"
        android:name="com.example.jaloliddinhaykaligavertualsayohat.fragment.MonumentDataFragment"
        android:label="fragment_monument_data"
        tools:layout="@layout/fragment_monument_data" />
    <fragment
        android:id="@+id/personDataFragment"
        android:name="com.example.jaloliddinhaykaligavertualsayohat.fragment.PersonDataFragment"
        android:label="fragment_person_data"
        tools:layout="@layout/fragment_person_data" />
</navigation>